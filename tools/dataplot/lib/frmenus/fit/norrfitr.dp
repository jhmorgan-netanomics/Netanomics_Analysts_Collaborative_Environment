. THIS IS DATAPLOT MACRO    NORRFITR.DP
. PURPOSE--FIT JIM NORRIS DATA FOR RUN (= SUBSET) RUNID
. INPUT--RUNID
.
FIT Y^RUNID X^RUNID
LET PRED^RUNID = PRED
LET RES^RUNID = RES
.
LET INT^RUNID = A0
LET SLOPE^RUNID = A1
LET RESSD^RUNID = RESSD
LET N^RUNID = NUMBER Y^RUNID
.
SET WRITE DECIMALS 6
PRINT RUNID N^RUNID INT^RUNID SLOPE^RUNID RESSD^RUNID
