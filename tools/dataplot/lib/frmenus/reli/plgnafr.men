AN FE
LET SIGMA = _1
LET ZLOC = 0
@IF 2 0 LET ZLOC = _2
LET ZSCALE = 1
@IF 3 0 LET ZSCALE = _3
LET ZZZZSTAR = 0
@IF 4 0 LET ZZZZSTAR = _4
LET ZINC = 0.1
@IF 7 0 LET ZINC = _7
@IF 5 0 LET ZZZZAFR = LGNAFR(ZZZZSTAR, _5 ,ZLOC,ZSCALE)
@IF 5 0 PLOT ZZZZAFR _5
@IF 6 0 PLOT LGNAFR( ZZZZSTAR,ZZ,SIGMA,ZLOC,ZSCALE) FOR ZZ = _6 ZINC _8
@IF 9 0 LET _9 = YPLOT
