IF NLABTEMP > 5
   SET BOB OFF
   LET IFLAG10 = 0
END OF IF
LET LABDIST = DISTINCT _2 _19
LET LABDIST = SORT LABDIST
LET ICNT = 0
LET NMETH = 0
IF IFLAGD = 1
   IF IFLAG1 = 1
      LET ICNT = ICNT + 1
      LET STRING SX^ICNT = MP
      LET NMETH = NMETH + 1
   END OF IF
   IF IFLAG2 = 1
      LET ICNT = ICNT + 1
      LET STRING SX^ICNT = ModifiedCR()MP
      LET NMETH = NMETH + 1
