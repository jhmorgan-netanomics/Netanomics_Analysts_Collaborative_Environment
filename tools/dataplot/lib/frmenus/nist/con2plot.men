   IF IFLAG4 = 1
      LET ICNT = ICNT + 1
      LET STRING SX^ICNT = DSLCR()Original
      LET ICNT = ICNT + 1
      LET STRING SX^ICNT = DSLCR()HHD
      LET ICNT = ICNT + 1
      LET STRING SX^ICNT = DSLCR()Bootstrap
      LET NMETH = NMETH + 3
   END OF IF
   IF IFLAG5 = 1
      LET ICNT = ICNT + 1
