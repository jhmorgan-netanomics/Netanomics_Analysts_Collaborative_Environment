   IF IFLAG10 = 1
      LET ICNT = ICNT + 1
      LET STRING SX^ICNT = BOB
      LET NMETH = NMETH + 1
   END OF IF
   LOOP FOR K = 1 1 NLABTEMP
      LET ICNT = ICNT + 1
      LET IVAL = ZZLABID(K)
      LET STRING SX^ICNT = ^IVAL
   END OF LOOP
ELSE
   LOOP FOR K = 1 1 NLABTEMP
      LET ICNT = ICNT + 1
      LET IVAL = ZZLABID(K)
      LET STRING SX^ICNT = ^IVAL
   END OF LOOP
   IF IFLAG1 = 1
      LET ICNT = ICNT + 1
      LET STRING SX^ICNT = MP
