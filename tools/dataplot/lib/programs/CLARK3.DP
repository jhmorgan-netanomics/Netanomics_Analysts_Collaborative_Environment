LET FUNCTION H9 = 30240*X-80640*X**3+48384*X**5-9216*X**7+512*X**9
LET FUNCTION H10 = -30240+302400*X**2-403200*X**4+161280*X**6...
-23040*X**8+1024*X**10
.
LET N = NUMBER X
LET X0 = 1 FOR I = 1 1 N
.
LET KMAX = 10
LOOP FOR K = 1 1 KMAX
   LET X^K = H^K * SQRT(GAMMA(K+1)/2**K)
   LET Z^K = EPI * X^K
END LOOP
.
LET Y2 = Y/EPI
.
. LOOP FOR K = 1 1 KMAX
.    FIT Y2 X1 TO X^K
.    LET RSD(K) = RESSD
